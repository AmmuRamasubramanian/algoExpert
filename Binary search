array = [0, 1, 21, 33, 45, 45, 61, 71, 72, 73]
target = 33
def BinarySearch(array,target):
    l=0
    r=len(array)-1
    while(r>=l):
        m=(l+r)//2
        if array[m]==target:
            break
        elif array[m]>target:
            r=m-1
        elif array[m]<target:
            l=m+1
    return m
print(BinarySearch(array,target))
